{
  "name": "linter-markdown",
  "main": "lib/index.js",
  "version": "1.3.0",
  "description": "Lint markdown on the fly, using remark-lint",
  "scripts": {
    "lint": "remark -u remark-lint README.md LICENSE.md"
  },
  "keywords": [],
  "activationCommands": {},
  "repository": "https://github.com/AtomLinter/linter-markdown",
  "license": "MIT",
  "engines": {
    "atom": "^1.0.0"
  },
  "dependencies": {
    "remark": "^3.0.0",
    "remark-lint": "^2.1.0",
    "atom-package-deps": "^3.0.6"
  },
  "devDependencies": {
    "eslint": "^1.10.3",
    "babel-eslint": "^4.1.6",
    "eslint-config-airbnb": "^3.0.0"
  },
  "eslintConfig": {
    "rules": {
      "comma-dangle": [
        2,
        "never"
      ],
      "no-console": 0
    },
    "extends": "airbnb/base",
    "parser": "babel-eslint",
    "globals": {
      "atom": true
    },
    "env": {
      "es6": true,
      "node": true
    }
  },
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  }
}
